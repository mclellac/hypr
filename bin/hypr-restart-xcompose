#!/bin/bash
#
# Restarts the fcitx5 input method editor, which handles XCompose.

set -euo pipefail

#######################################
# Main function to restart the fcitx5 service.
# Globals:
#   None
# Arguments:
#   None
# Outputs:
#   None
#######################################
main() {
  hypr-restart-app fcitx5
}

main
